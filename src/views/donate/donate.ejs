<script>
  $("a.side-nav-donate").addClass("active");
  document.getElementById("a-nav-donate").className += " active";


  $(document).ready(function() {
    $('#donate-paypal').on('click', function() {
      if (_userId) {
        $('#create-donate-modal').modal();
        $('.field-validation').hide();
      } else {
        $('#authen-modal').modal();
      }
    })
    $('#submit-donate').on('click', function() {
      let amount = $('#amount-donate').val(),
        content = $('#content-donate').val();
      if (amount && content) {
        $('.field-validation').hide();
        $('#form-donate').submit();
      } else {
        $('.field-validation').show();
      }


    })
  });
</script>