  <div class="modal fade" id="create-donate-modal" tabindex="-1" role="dialog" style="display: none;" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
      <div class="modal-content">
        <div class="block block-themed block-transparent">
          <div class="block-header bg-primary-dark">
            <h3 class="block-title font-w600">Ủng hộ MYCLASS</h3>
            <div class="block-options">
              <button type="button" class="btn-block-option" data-dismiss="modal" aria-label="Close">
                <i class="si si-close"></i>
              </button>
            </div>
          </div>
          <div class="block-content">
            <form method="post" action="/pay" id="form-donate">
              <input class="form-control form-control-lg" style="display: none;" name="user_name"value="<% if(user){ %><%- `${user.firstname} ${user.lastname}` %> <% } %>">
              <input class="form-control form-control-lg"  style="display: none;" name="user_id" value="<% if(user){ %><%=user._id %><% } %>">
              <input class="form-control form-control-lg"  style="display: none;" name="avatar" value="<% if(user){ %><%=user.avatar %><% } %>">
              <span class="field-validation text-danger" data-valmsg-for="Body" data-valmsg-replace="true">Vui lòng nhập đủ thông tin</span>
              <div class="row">
                <div class="col-md-12">
                  <div class="form-group">
                    <label for="Title">Số tiền ủng hộ(USD)</label>
                    <input class="form-control form-control-lg" type="number" id="amount-donate" style="width: 180px" name="amount">
                  </div>
                </div>
                <div class="col-md-12">
                  <div class="form-group">
                    <label for="Body">Nội dung</label>
                    <textarea class="form-control form-control-lg" cols="4" data-val="true" id="content-donate" name="content" placeholder="Nội dung" rows="4"></textarea>

                  </div>
                </div>

                <div class="col-md-12">
                  <div class="text-right">
                    <button type="button" class="btn btn-link" data-dismiss="modal">Hủy bỏ</button>
                    <button type="button" id="submit-donate" class="btn btn-success  min-width-100"><i class="fas fa-cloud-upload"></i> Ủng hộ</button>
                  </div>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>

  </div>

